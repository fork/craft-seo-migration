{
    "name": "fork/craft-seo-migration",
    "description": "Migrate from ether/seo to Retour and SEOMatic.",
    "type": "craft-plugin",
    "license": "mit",
    "support": {
        "email": "obj@fork.de",
        "issues": "https://github.com/fork/craft-seo-migration/issues?state=open",
        "source": "https://github.com/fork/craft-seo-migration",
        "docs": "https://github.com/fork/craft-seo-migration",
        "rss": "https://github.com/fork/craft-seo-migration/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.12.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "fork\\craftseomigration\\": "src/"
        }
    },
    "extra": {
        "handle": "seo-migration",
        "name": "SEO Migration",
        "developer": "Fork Unstable Media GmbH",
        "documentationUrl": "https://github.com/fork/craft-seo-migration",
        "class": "fork\\craftseomigration\\SeoMigration"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

